	"devDependencies": {
		"@babel/preset-env": "^7.13.9",
		"@types/jest": "^26.0.20",
		"babel-jest": "^26.6.3",
		"jest": "^26.6.3",
		"parcel": "^2.0.0-beta.2",
		"prettier": "^2.2.1"
	},
	"browserslist": [
		"last 3 and_chr versions",
		"last 3 chrome versions",
		"last 3 opera versions",
		"last 3 ios_saf versions",
